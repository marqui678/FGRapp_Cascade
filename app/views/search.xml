<Alloy>
	<Window id="searchWindow" theme="FGRThemeWithoutActionBar">
		<View>
			<View id="searchView">
				<Button id="backBtn" onClick="cancelSearch"  />
				<SearchBar id="searchBar"  showCancel="false" />
			</View>
			<View id="curLoc" onClick="useCurrentLoc" layout="horizontal">
				<ImageView id="curLocImage" image="/images/ic_navigation.png" />				
				<Label id="curLocLabel">Current location</Label>		
			</View>			
		</View>
	</Window>
</Alloy>